<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="contact" package="com.axelor.apps.contact.db"/>

  <entity name="Email">
    	<string name="emailAddress" title="Email" unique="true" required="true" search="emailAddress"/>
    	<boolean name="primary" column="is_primary"/>
    		<many-to-one  name="student" ref="com.axelor.apps.contact.db.Student" required="true"/>
  </entity>
</domain-models>