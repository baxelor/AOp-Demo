	  </action-view>

		<menuitem name="menu-contact-configure-circle" parent="menu-contact-configure" title="Circle" action="contact.configure.circle"/>   
		  <action-view name="contact.configure.circle"  title="Circles" model="com.axelor.apps.contact.db.Circle">
		  	<view type="grid" name="contact-circle-grid"/>
		  	<view type="form" name="contact-circle-form"/>
		  </action-view>
		  
		  <menuitem name="menu-contact-configure-department" parent="menu-contact-configure" title="Department" action="contact.configure.department"/>   
		  <action-view name="contact.configure.department"  title="Departments" model="com.axelor.apps.contact.db.Departments">
		  	<view type="grid" name="contact-department-grid"/>
		  	<view type="form" name="contact-department-form"/>
		  </action-view>
