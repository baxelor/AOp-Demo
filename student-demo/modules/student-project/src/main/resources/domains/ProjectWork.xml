<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="project" package="com.axelor.apps.project.db"/>

  <entity name="ProjectWork" table="PROJECT_WORK" cacheable="true">
	  <string name="name" required="true"/>
	  <date name="date" column="work_date" required="true"/>
	  <time name="hours" column="work_hours" title="Time Spent"/>
	  	<many-to-one  name="task" ref="com.axelor.apps.project.db.ProjectTask" required="true"/>
	  		<many-to-one  name="user" ref="com.axelor.auth.db.User" column="user_id"/>
  </entity>
</domain-models>